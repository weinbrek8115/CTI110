#Rectangle 1

length1=int(input("Enter length for rectangle 1: "))
width1=int(input("Enter width for rectangle 1: "))

#Rectangle 2
length2=int(input("Enter length for rectangle 2: "))
width2=int(input("Enter width for rectangle 2: "))

#Calculate area
area1=length1*width1
area2=length2*width2

#Which one has the larger area?
if area1>area2:
    print ("Rectangle 1 has the larger area")
elif area2>area1:
    print ("Rectangle 2 has the larger area")
else:
    print ("Both rectangles have equal area")
            
