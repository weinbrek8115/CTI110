#P3HW1:Age Classifier

#Karla Weinbrenner

#1 March 2018


age=int(input("Enter the person's age: "))
def main ():
    if age <=1:
        print ("You are an infant")

    elif age >1 and age <=13:
        print ("You are a child")

    elif age >13 and age <20:
        print ("Your are a teenager")

    elif age >=20:
        print ("You are an adult")

main ()
